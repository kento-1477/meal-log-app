{
  "uid": "shadowDiff",
  "title": "Shadow Diff Monitoring",
  "tags": ["shadow", "nutrition"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "refresh": 1,
        "hide": 0
      },
      {
        "name": "field",
        "type": "custom",
        "label": "Field",
        "query": "dkcal,dp,df,dc,rel_p,rel_f,rel_c",
        "current": {
          "selected": true,
          "text": "All",
          "value": [".*"]
        },
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      },
      {
        "name": "env",
        "type": "custom",
        "label": "Environment",
        "query": "prod,stg,dev,local",
        "current": {
          "selected": true,
          "text": "All",
          "value": [".*"]
        },
        "includeAll": true,
        "multi": true,
        "allValue": ".*"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "Record diff P95 (abs)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, env, field) (rate(meal_log_shadow_diff_abs_bucket{field=~\"$field\", env=~\"$env\"}[5m])))",
          "legendFormat": "{{field}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Record diff P95 (rel)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, env, field) (rate(meal_log_shadow_diff_rel_bucket{field=~\"$field\", env=~\"$env\"}[5m])))",
          "legendFormat": "{{field}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Daily diff P95 (abs)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, env, field) (rate(meal_log_shadow_daily_diff_abs_bucket{field=~\"$field\", env=~\"$env\"}[15m])))",
          "legendFormat": "{{field}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Daily diff P95 (rel)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, env, field) (rate(meal_log_shadow_daily_diff_rel_bucket{field=~\"$field\", env=~\"$env\"}[15m])))",
          "legendFormat": "{{field}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 5,
      "title": "Record breach count (1h)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (field, env) (increase(meal_log_shadow_diff_breach_total{field=~\"$field\", env=~\"$env\"}[1h]))",
          "legendFormat": "{{field}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 6,
      "title": "Daily breach count (1d)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "$datasource"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (field, env) (increase(meal_log_shadow_daily_diff_breach_total{field=~\"$field\", env=~\"$env\"}[1d]))",
          "legendFormat": "{{field}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    }
  ]
}
